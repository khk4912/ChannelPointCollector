{
    "manifest_version": 2,
    "name": "ChannelPointCollector",
    "description": "Collect Twitch channel point automaticly.",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "storage",
        "https://www.twitch.tv/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}